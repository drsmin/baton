                </div><!-- row -->
            </nav>
            <div class="mr30"></div>
            <form action="/sel/selReg5">
                <div class="form-group row">
                    <label for="svcCtnt" class="col-sm-2 col-form-label">이미지 등록</label>
                    <div class="col-sm-10">
                        <button type="button" class="btn btn-info" id="imgRegPop">등록</button>
                    </div>
                </div>
                <div class="form-group row">
                    <label for="svcCtnt" class="col-sm-2 col-form-label">동영상 등록</label>
                    <div class="col-sm-10">
                        <button type="button" class="btn btn-info" id="movRegPop">등록</button>
                    </div>
                </div>
                <div class="form-group row">
                    <div class="col-sm-6 text-left">
                        <button type="button" class="btn btn-secondary" onclick="history.back()">이전</button>
                    </div>
                    <div class="col-sm-6 text-right">
                        <button type="submit" class="btn btn-primary">다음단계</button>
                    </div>
                </div>
            </form>
            
        </div> <!-- Container -->
    </main>

<% include sys/inc/_footer %>
</body>
</html>